<Bindings>
    <Binding name="PAPAGREET_GREET" header="PAPAGREET" runOnUp="true">
        PapaGreet_SendGreeting()
    </Binding>
    <Binding name="PAPAGREET_GOODBYE" header="PAPAGREET" runOnUp="true">
        PapaGreet_SendGoodbye()
    </Binding>
    <Binding name="PAPAGREET_CANCEL" header="PAPAGREET" runOnUp="true">
        PapaGreet_CancelLeave()
    </Binding>
    <Binding name="PAPAGREET_MENU" header="PAPAGREET" runOnUp="true">
        PapaGreet_ToggleMenu()
    </Binding>
</Bindings>

